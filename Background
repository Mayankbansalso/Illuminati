<body>
    <h1>Random Illuminati Images</h1>
    <img id="illuminatiImage" src="https://via.placeholder.com/300" alt="Illuminati">
    <br>
    <button onclick="showRandomImage()">Show Random Image</button>

    <script>
        const images = [
            'https://upload.wikimedia.org/wikipedia/commons/thumb/e/e1/Eye_of_Providence.svg/1200px-Eye_of_Providence.svg.png',
            'https://www.publicdomainpictures.net/pictures/290000/nahled/illuminati-pyramid.jpg',
            'https://upload.wikimedia.org/wikipedia/commons/thumb/d/d2/MasonicEye.jpg/800px-MasonicEye.jpg',
            'https://cdn.pixabay.com/photo/2015/04/21/09/24/eye-732227_1280.jpg'
        ];

        function showRandomImage() {
            const randomIndex = Math.floor(Math.random() * images.length);
            document.getElementById('illuminatiImage').src = images[randomIndex];
        }
    </script>
</body>
